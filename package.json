{
  "name": "@sprintly/react-mentions",
  "version": "0.1.10",
  "description": "React mentions input",
  "main": "lib/index.js",
  "scripts": {
    "test": "script/test",
    "transpile": "jsx --harmony --no-cache-dir -x jsx src lib",
    "dist": "script/dist",
    "start": "cd gh-pages && webpack-dev-server --port 9090 --d --hot --inline --progress --colors",
    "bump-patch": "mversion patch -m",
    "bump-minor": "mversion minor -m",
    "bump-major": "mversion major -m",
    "predist": "npm run transpile",
    "pretranspile": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/effektif/react-mentions.git"
  },
  "keywords": [
    "react",
    "mentions",
    "react-component"
  ],
  "author": "Jan-Felix Schwarz",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/effektif/react-mentions/issues"
  },
  "homepage": "https://github.com/effektif/react-mentions",
  "devDependencies": {
    "babelify": "^6.1.1",
    "browserify": "4.2.3",
    "browserify-shim": "3.6.0",
    "bundle-loader": "0.5.1",
    "chai": "1.10.0",
    "envify": "1.2.0",
    "grunt": ">=0.4.1",
    "grunt-contrib-copy": ">=0.4.1",
    "grunt-contrib-less": "~1.0.0",
    "grunt-gh-pages": "~0.9.1",
    "jsx-loader": ">=0.12.0",
    "karma": "0.12.16",
    "karma-browserify": "^0.2.1",
    "karma-chrome-launcher": "0.1.4",
    "karma-cli": "0.0.4",
    "karma-coverage": "~0.2.7",
    "karma-firefox-launcher": "0.1.3",
    "karma-junit-reporter": "~0.2.2",
    "karma-mocha": "0.1.3",
    "karma-phantomjs-launcher": "~0.1.4",
    "less": "~2.3.1",
    "less-loader": "^2.2.0",
    "mocha": "1.20.1",
    "mversion": "^1.10.0",
    "react": "~0.13.0",
    "react-hot-loader": "^1.2.5",
    "react-router": "^0.13.2",
    "react-tools": "~0.13.0",
    "reactify": "0.15.x",
    "rf-release": "0.3.2",
    "sinon": "~1.12.1",
    "sinon-chai": "~2.6.0",
    "uglify-js": "2.4.15",
    "webpack": "1.4.5",
    "webpack-dev-server": "1.6.5"
  },
  "peerDependencies": {
    "react": ">=0.12.0"
  },
  "browserify-shim": {
    "react": "global:React",
    "react/addons": "global:React"
  }
}
